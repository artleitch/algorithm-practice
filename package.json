{
  "name": "js-gym",
  "version": "0.0.1",
  "description": "A place to practice algorithm problems that I find around the web",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "build": "tsc",
    "watch": "npm-watch",
    "dev": "tsc -w --onSuccess \"npm run coverage\"",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm t"
  },
  "watch": {
    "coverage": {
      "patterns": [
        "src"
      ],
      "quiet": true,
      "extensions": "ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artleitch/js-gym.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artleitch/js-gym/issues"
  },
  "homepage": "https://github.com/artleitch/js-gym#readme",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "jsdoc": "^3.6.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@types/node": "^14.14.6",
    "npm-watch": "^0.7.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.5"
  }
}
